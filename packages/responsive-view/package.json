{
  "name": "@meta-ultra/responsive-view",
  "private": false,
  "version": "0.1.0",
  "description": "",
  "scripts": {
    "clean": "rimraf ./dist",
    "dev": "pnpm run \"/^dev:.*/\"",
    "dev:src": "rollup --watch -c ./rollup.config.js",
    "dev:types": "tsc --watch -P ./tsconfig.types.json",
    "build": "pnpm clean && cross-env NODE_ENV=production pnpm run \"/^build:.*/\"",
    "build:src": "rollup -c ./rollup.config.js",
    "build:types": "tsc -P ./tsconfig.types.json"
  },
  "unpkg": "./dist/responsive-view.umd.js",
  "module": "./dist/responsive-view.esm.js",
  "main": "./dist/responsive-view.cjs.js",
  "typings": "./dist/types/index.d.ts",
  "sideEffects": false,
  "files": [
    "./dist"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "next.js",
    "react",
    "app router",
    "router",
    "routing"
  ],
  "author": "fsjohnhuang",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/plugin-transform-react-constant-elements": "^7.22.5",
    "@babel/plugin-transform-runtime": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@types/lodash-es": "^4.17.7",
    "@types/node": "^20.2.5",
    "@types/react": "^18.2.9",
    "babel": "^6.23.0",
    "babel-plugin-import": "^1.13.8",
    "cross-env": "^7.0.3",
    "react-dom": "18.2.0",
    "rimraf": "^5.0.1",
    "tsc-alias": "^1.8.6",
    "typescript": "^5.1.3",
    "rollup": "^4.1.4",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-uglify": "^6.0.4"
  },
  "dependencies": {
    "lodash-es": "^4.17.21"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  }
}
