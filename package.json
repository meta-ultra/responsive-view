{
  "name": "responsive-view",
  "version": "1.0.0",
  "description": "",
  "module": "./dist/esm/index.js",
  "jsnext:main": "./dist/esm/index.js",
  "types": "./dist/types",
  "scripts": {
    "prepare": "husky install",
    "_build:cjs": "webpack --config webpack.config.js",
    "build:esm": "tsc && tsc-alias",
    "build:types": "cross-env-shell OUT_DIR=./dist/types/ \"tsc --outDir $OUT_DIR --declaration --emitDeclarationOnly && tsc-alias --outDir $OUT_DIR\"",
    "clean": "rimraf ./dist",
    "build": "pnpm clean && pnpm run \"/^build:.*/\"",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "author": "fsjohnhuang",
  "license": "ISC",
  "lint-staged": {
    "*.vue": [
      "prettier --write",
      "eslint",
      "stylelint --fix"
    ],
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint"
    ],
    "*.{htm,html,css,les,scss,sass}": [
      "prettier --write",
      "stylelint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/plugin-transform-react-constant-elements": "^7.22.5",
    "@babel/plugin-transform-runtime": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@storybook/addon-essentials": "7.0.20",
    "@storybook/addon-interactions": "7.0.20",
    "@storybook/addon-links": "7.0.20",
    "@storybook/blocks": "7.0.20",
    "@storybook/react": "7.0.20",
    "@storybook/react-webpack5": "7.0.20",
    "@storybook/testing-library": "0.0.14-next.2",
    "@types/lodash-es": "^4.17.7",
    "@types/node": "^20.2.5",
    "@types/react": "^18.2.9",
    "@types/webpack-env": "^1.18.1",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "babel-loader": "^9.1.2",
    "commitizen": "4.3.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.12",
    "husky": "6.0",
    "lint-staged": "^13.2.2",
    "prettier": "2.8.8",
    "react-dom": "18.2.0",
    "rimraf": "^5.0.1",
    "storybook": "7.0.20",
    "stylelint": "^15.7.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-order": "^6.0.3",
    "tsc-alias": "^1.8.6",
    "typescript": "^5.1.3",
    "webpack": "^5.86.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.22.5",
    "core-js": "^3.31.0",
    "core-js-pure": "^3.31.0",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  }
}
